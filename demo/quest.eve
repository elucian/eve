#driver "sw"

make msg @ S;
make var âˆˆ Z;
read(var,"var:>");
quest var:
  case 0:
    alter msg := "zero";
  case 1:
    alter msg := "one";
  case (1,3,5,7,9):
    alter msg := "odd";
  case [1..3]:
    alter msg := "small";
  case [4..9]:
    msg := "large";
cover:
  msg := "Undexpected";
ready;

print msg;

over.