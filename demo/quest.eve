****************************
** quest statement demo
****************************
#driver "sw"

String: msg
Integer: var

read (var,"var:>")
quest var
  ?:0 do
    msg := "zero"
  ?:1 do
    msg := "one"
  ?:(1,3,5,7,9) do
    msg := "odd"
  ?:[1..3] do
    msg := "small"
  ?:[4..9]:
    msg := "large"
other
  msg := "Undexpected"
done

print msg
