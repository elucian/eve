****************************
** quest statement demo
****************************
method main():
  String:  msg ;  ** reference to String
  Integer: var ;  ** reference to Integer
process
  print ("Enter a number: 1..9");
  read  (var,"var:>");
  quest var:
    match 2,4,6,8 do
      msg := "even";
    match (1,3,5,7,9) do
      msg := "odd";
  none
    msg := "0: Unexpected";
  done;
  print msg;
return;