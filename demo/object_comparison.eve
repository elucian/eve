#object comparison demo
driver object_compare:

-- todo
global
    set o = n = 1: Integer; -- context variable
process  
    ** equal values and types
    expect  o  == n; 
    expect  not (o != n);     
    
    ** not the same location
    expect  not (o is n); 
    expect  o is not n;     
   
    ** alter one value        
    let n := 2; 

    ** equal values and types
    expect  o  != n; 
    expect  not (o == n);         
return;