# multiple line demo
driver multi_line:

global
    set x: Integer;
    set a: HashTable;
process
    ** multi-row expression
    let x := 1 + 
         2 +
        (3 + 4)
       + 5;
       
    ** multi-row literal
    let a := {
         (1:2)
        ,(3:4)
        ,(5:6)
    };
return;

/* This is why, Eve use semicolon for every statement,
   to be able to write multi-line statements without effort. */