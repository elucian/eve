** example of string concatenation
driver string_concat:

global  
  String str = "";  -- empty String
process
  ** set string value using different operators
  str := " this " + " string"; expect str == " this  string";
  str := " this " - " string"; expect str == "this string";  
  str := "this/" / " string"; expect str == "this/string";  
  str := "this"  .   string ; expect str == "this/string";  
return;