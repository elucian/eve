**********************************
** example of collection iteration
**********************************
module while:

method main():
  Array  : test = ['a','b','c','d','e'];
  Symbol : element; 
  Natural: i := 1;
trial
  while i <= test.length() do
    element := test[i]; // transfer a reference to symbol   
    ** shortcut 
    when (element >= 'c') do
      write (element);
      write (',') if (element <> 'e');
    done;
  repeat;
return;

module. // end of module