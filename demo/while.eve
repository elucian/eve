**********************************
** example of collection iteration
**********************************
method main():
   Array  : test := ['a','b','c','d','e'];
   String : element; 
   Natural: i := 1;
process
  while i <= test.length() do
    element :: test[i]; -- transfer a reference    
    ** shortcut 
    when (element >= 'c') do
       write (element);
       write (',') if (element <> 'e');
    done;
  repeat;
return;
